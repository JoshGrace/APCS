
public class ting {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] m = new int[4][4];
		for(int[] b : m){
			for(int v : b){
				v = 0;
			}
		}
		int n = m.length;
		for(int i = 1; i < n-1; i++){
			for(int j = 1; j < n-1; j++){
				m[i][j] = 1;
			}
		}
		for(int[] b : m){
		for(int v : b){
			System.out.println(v);
		}
	}
	}
}
