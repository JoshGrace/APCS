package com.eimacs.lab05;

/**
 *
 * @author |Your name|
 * @version 1.0 |Today's date|
 */
public class MoveForward extends Action{
    int space = 0;
    public MoveForward(int space){
        this.space= space;
    }
    public String toString(){
        return "Forward " + Integer.toString(space);
    }
} 
