
public class eimacs {

	public static void main(String[] args) {
		boolean[] p = { false, true, true, false }; 

		double[] q = { 1.0, 2.0 }; 

		double y = 0; 

		for ( double d : q ) 
		{ 
		  for ( boolean b : p ) 
		  { 
		    if ( b )  
		      y += d; 
		  } 
		} 
		System.out.println(y);
	
	}
}
