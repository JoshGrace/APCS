public class EImacs {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(pickOne( 2, 2, 3));
	}
	public static int pickOne( int a, int b, int c ) 
	  { 
	    if ( a < b && (b < c || a < c) ) 
	      return a; 

	    if ( b < c ) 
	      return b; 

	    return c;     
	  }
}
