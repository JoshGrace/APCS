public class EImacs {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] x = { "a", "b", "c", "d" };
		String[] y = { "membership" };
		String[] z = { };
		int[] a = { 1, 2, 2, 3, 3, 3, 4, 4, 4, 4 };
		System.out.println( arrayToString( Z ) );
	}
	 public static String arrayToString( String[] sArr ) 
	  { 
	    String s = "[ "; 
	    for ( int i = 0 ; i < sArr.length ; i++ ) 
	    { 
	      s += sArr[ i ]; 
	      if ( i + 1 < sArr.length ) 
	        s += ", "; 
	    } 
	    s += " ]"; 
	    return s; 
	  }
}
