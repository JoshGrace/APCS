public class EImacs {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//String[] thi = {"v", "c"};
		//String[] thin = {"k", "c", "l"};
		int[] a = { 1, 11, 111, 2, 22, 222 };
		System.out.println( checkIntArray( a ));
	}
	public static boolean checkIntArray( int[] a ) 
	  { 
	    int i = 1; 
	    while ( i < a.length ) 
	    { 
	      if ( a[ i - 1 ] > a[ i ] ) 
	        return false; 

	      i++; 
	    } 

	    return true; 
	  }
}
