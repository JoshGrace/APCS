public class EImacs {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(reverseString( "forward" ));
	}
	public static String reverseString( String s ) 
	  { 
	    String str = ""; 
	    for ( int i = 0 ; i < s.length() ; i++ ) 
	    { 
	      str = s.substring( i, i + 1 ) + str; 
	    } 

	    return str; 
	  }
}
