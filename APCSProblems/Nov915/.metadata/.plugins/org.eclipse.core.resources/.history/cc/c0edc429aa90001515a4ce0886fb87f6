public class EImacs {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(reverseString( "forward" ));
	}
	public static String reverseString( String s ) 
	  { 
	    String str = ""; 
	    for ( int i = s.length() ; i > 0 ; i-- ) 
	    { 
	      str += s.substring( i - 1, i ); 
	    } 

	    return str; 
	  }
}
